<?xml version="1.0"?>
<config>
    <modules>
        <MpsSistemi_Core>
            <version>0.1.0</version>
        </MpsSistemi_Core>
    </modules>
    <global>

        <blocks>
            <mpscore>
                <class>MpsSistemi_Core_Block</class>
            </mpscore>
        </blocks>
        <helpers>
            <mpscore>
                <class>MpsSistemi_Core_Helper</class>
            </mpscore>            
        </helpers>        
        <models>    
            <mpscore> 
                <class>MpsSistemi_Core_Model</class>                
            </mpscore>
        </models>            
    </global> 
    <adminhtml>
        <layout>
            <updates>
                <mpscore>
                    <file>mps/mpscore.xml</file>
                </mpscore>
            </updates>
        </layout>
    </adminhtml>
    
</config>